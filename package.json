    "cpp",
    "c++"
  ],
  "author": "Arthur Mendes",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  }
}

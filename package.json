{
  "name": "demangler-js",
  "version": "0.1.6",
  "description": "C++ name demangler written in javascript",
  "repository": "https://github.com/arthurmco/demangler-js",
  "main": "src/index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha test/",
